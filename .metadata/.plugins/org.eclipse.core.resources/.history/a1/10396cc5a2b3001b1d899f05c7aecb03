package com.employeeapp;

class Cal {
	
	public static int add(int a, int b) {
		System.out.println("public static int add(int a, int b) ");
		return a+b;
	}
	
	public static Integer add(Integer a, Integer b) {
		System.out.println("public static Integer add(Integer a, Integer b)");
		return a+b;
	}
	
//	public static long add(long a, int b) {
//		System.out.println("public static long add(long a, int b) ");
//		return a+b;
//	}
//
//	public static long add(int a, long b) {
//		System.out.println("public static long add(int a, long b)");
//		return a+b;
//		
//	}
}
public class DemoOverloading {
	public static void main(String[] args) {
		long result = Cal.add(2L, 1); 
		
		System.out.println(result);
	}

}
