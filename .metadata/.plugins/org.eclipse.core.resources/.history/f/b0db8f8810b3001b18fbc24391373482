package com.day2.session2.stackapp;
class Stack{
	private int data[];
	private final int SIZE;
	private int index;
	
	public Stack() {
		SIZE=5;
		index=-1;// stack is empty
	}
	public boolean isEmpty() {
		return (index==-1)?true:false;
	}
	
}
public class DemoStack {

	public static void main(String[] args) {
		
	}
}
