package com.employeeapp;

class Cal {
	public static int add(int a, int b) {
		System.out.println("public static int add(int a, int b)");
		return a+b;
	}

	public static int add(int a, int b, int c) {
		System.out.println("public static int add(int a, int b, int c)");
		return a+b+c;
		
	}

}

public class DemoOverloading {

	public static void main(String[] args) {
		int result = Cal.add(2, 1,12);// it called compile time poly... 
		//compiler alreay decicided which method need to be called at run time
		System.out.println(result);
	}

}
