package com.day2.session1.inher;

class A{
	int i=10;
	public void methodA() {
		System.out.println(i);
	}
}

class B extends A{
	int i=88;
	public void methodB() {
		methodA();
		System.out.println(i);
		
	}
}
public class DemoInheri {
	
	public static void main(String[] args) {
		B  b=new B();
		b.methodB();
	}

}
