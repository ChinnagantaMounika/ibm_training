package com.day2.session1.inher;

class A{
	int i=10;
	public void methodA() {
		System.out.println(i);
	}
}

class B extends A{
	int j=88;
	public void methodB() {
		methodA();
		System.out.println(i);
		System.out.println(j);
	}
}
public class DemoInheri {
	
	public static void main(String[] args) {
		
	}

}
