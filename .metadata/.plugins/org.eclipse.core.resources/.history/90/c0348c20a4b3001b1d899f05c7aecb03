package com.employeeapp;

class Cal {
	public static int add(int b, int ...a) {
		int sum=0;
		for(int i=0;i<a.length; i++) {
			sum+=a[i];
		}
		return sum;
	}
}
public class DemoOverloading {
	public static void main(String[] args) {
		
		int sum=Cal.add(5,6,7,7);
		System.out.println(sum);
		
	}

}
