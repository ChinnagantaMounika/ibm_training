package com.day2.session1.inher;
class Point{
	private int x, y;
	public Point(int x, int y) {
		this.x = x;
		this.y = y;
	}
	public int getX() {
		return x;
	}
	public int getY() {
		return y;
	}
	public void printPointDetails() {
		System.out.println("{x,y}:"+ x+","+ y);
	}
	
}
public class DemoTraingleProblem {
	
	public static void main(String[] args) {
		
	}

}
